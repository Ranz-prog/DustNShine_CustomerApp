<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dustnshine">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DustNShine">
        <activity
            android:name=".ui.activities.ActivitySeeAllRecommendations"
            android:exported="false" />
        <activity
            android:name=".ui.activities.ActivityFeedBack"
            android:exported="false" />
        <activity
            android:name=".ui.MessageBox"
            android:exported="false" />
        <activity
            android:name=".ui.splash_screen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activities.ActivityCompanyDetails"
            android:exported="true" />
        <activity
            android:name=".ui.activities.ActivitySignIn"
            android:exported="true" />
        <activity
            android:name=".ui.activities.ActivitySignUp"
            android:exported="true" />
        <activity
            android:name=".ui.activities.ActivityManageAccount"
            android:exported="true" />
        <activity
            android:name=".ui.PersonalInfoPopLayout"
            android:exported="true" />
        <activity
            android:name=".ui.activities.ActivitySearchEmail"
            android:exported="true" />
        <activity
            android:name=".ui.activities.ActivityForgetPassword"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity android:name=".ui.activities.ActivityBookingHistory" />
        <activity android:name=".ui.activities.ActivityNotification" />
        <activity android:name=".ui.activities.ActivityCheckOut" />
        <activity android:name=".ui.ActivityTimeAndDate" />
    </application>

</manifest>